<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">

      <em:id>fat-free-firefox@oyenamit</em:id>
      <em:name>Fat-Free Fox</em:name>
      <em:description>Safeguard your private information by disabling privacy-violating features of Firefox and by enabling tracking protection from third-party ads. Keep your browser fat-free by disabling some of its unwanted features.</em:description>
      <em:version>3.0</em:version>
      <em:creator>oyenamit</em:creator>
      <em:iconURL>chrome://fat-free-firefox/skin/fat-free-firefox-icon.png</em:iconURL>
      <em:icon64URL>chrome://fat-free-firefox/skin/fat-free-firefox-icon64.png</em:icon64URL>
      <em:type>2</em:type>
      <em:bootstrap>true</em:bootstrap>
      <em:optionsType>2</em:optionsType>
      <em:optionsURL>chrome://fat-free-firefox/content/preferences.xul</em:optionsURL>
      <em:multiprocessCompatible>true</em:multiprocessCompatible>

      <!-- Firefox support -->
      <em:targetApplication>
          <Description>
              <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
              <em:minVersion>29.0</em:minVersion>
              <em:maxVersion>56.*</em:maxVersion>
          </Description>
      </em:targetApplication>

      <!-- US English localization -->
      <em:localized>
          <Description>
              <em:locale>en-US</em:locale>
              <em:name>Fat-Free Fox</em:name>
              <em:description>Safeguard your private information by disabling privacy-violating features of Firefox and by enabling tracking protection from third-party ads. Keep your browser fat-free by disabling some of its unwanted features.</em:description>
              <em:creator>oyenamit</em:creator>
              <em:translator>oyenamit</em:translator>
          </Description>
      </em:localized>

      <!-- Spanish localization -->
      <em:localized>
          <Description>
              <em:locale>es</em:locale>
              <em:name>Fat-Free Fox</em:name>
              <em:description>Salvaguarda su información privada deshabilitando caracterísicas que violan la privacidad de Firefox y habilitando protección contra el rastreo realizado por publicidad de terceros. Mantiene su navegador desgrasado deshabilitando algunas de sus características no deseadas.</em:description>
              <em:creator>oyenamit</em:creator>
              <em:translator>strel</em:translator>
          </Description>
      </em:localized>
  </Description>
</RDF>

